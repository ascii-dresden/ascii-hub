{
  "name": "asciii-gui",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start:de": "ng serve --aot --locale de --i18n-format xlf2 --i18n-file src/i18n/messages.de.xlf --missing-translation error",
    "build": "ng build --prod --aot && node build --en",
    "build:de": "ng build --prod --aot --locale de --i18n-format xlf2 --i18n-file src/i18n/messages.de.xlf --missing-translation error && node build --de",
    "test": "ng test",
    "lint": "ng lint",
    "i18n": "ng xi18n --outputPath src/i18n --i18nFormat=xlf2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.1",
