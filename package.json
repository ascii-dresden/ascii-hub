{
  "name": "asciii-gui",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start:en": "ng serve --aot",
    "start:de": "ng serve --aot --locale de --i18n-format xlf2 --i18n-file src/i18n/messages.de.xlf --missing-translation error",
    "build": "ng build --prod --aot && node build --en",
    "build:en": "ng build --prod --aot && node build --en",
    "build:de": "ng build --prod --aot --locale de --i18n-format xlf2 --i18n-file src/i18n/messages.de.xlf --missing-translation error && node build --de",
    "i18n": "ng xi18n --outputPath src/i18n --i18nFormat=xlf2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bootstrap": "^4.1.1",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "popper.js": "^1.14.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "archiver": "^2.1.1",
    "codelyzer": "^4.3.0",
    "minimist": "^1.2.0",
    "tslint": "^5.9.1",
    "typescript": "~2.5.3"
  }
}
