  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod && node build --en",
    "build:en": "ng build --prod && node build --en",
    "build:de": "ng build --prod --i18n-locale de --i18n-format xlf2 --i18n-file src/i18n/messages.de.xlf --i18n-missing-translation warn && node build --de",
    "i18n": "ng xi18n -c en",
    "i18n:en": "ng xi18n -c en",
    "i18n:de": "ng xi18n -c de",
    "test": "ng test",
    "test:sr": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
