    "@angular/http": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/router": "^5.2.1",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0",
    "core-js": "^2.5.3",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.0",
    "minimist": "^1.2.0",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "popper.js": "^1.13.0",
